# Example environment configuration for Hermis Protocol contracts
# Copy this file to .env and fill in the required values before running deployments.

# Private key for deployments (never commit your real key)
PRIVATE_KEY=0xYOUR_PRIVATE_KEY

# Optional local developer key (e.g., Anvil default account)
LOCAL_PRIVATE_KEY=0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80

# RPC endpoints
BASE_RPC_URL=https://base-mainnet.g.alchemy.com/v2/YOUR_KEY
BASE_SEPOLIA_RPC_URL=https://base-sepolia.g.alchemy.com/v2/YOUR_KEY

# Block explorer API keys (used for contract verification)
BASE_ETHERSCAN_API_KEY=your-basescan-api-key
ETHERSCAN_API_KEY=your-etherscan-api-key

# Optional custom settings
# DEPLOY_FROM_ADDRESS=0xYourDeployerAddress
# GAS_PRICE_GWEI=20
